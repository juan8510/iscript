<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${dtoPackageName}">
	<resultMap type="${dtoSimpleName}" id="RM_${dtoSimpleName}">
		${resultMap-results}
	</resultMap>
	<sql id="${dtoSimpleName}-coloumn-list">
		${coloumn-list}
	</sql>
	<insert id="batchInsert" parameterType="${dtoSimpleName}">
		INSERT INTO ${tableName}(
		<include refid="${dtoSimpleName}-coloumn-list" />
		)
		VALUES
		<foreach collection="list" item="item" index="index"
			separator=",">
			(${insert-values})
		</foreach>
	</insert>
	<update id="updateOne" parameterType="${dtoSimpleName}">
		UPDATE ${tableName} SET
		${update-values}
		WHERE ID=#{id}
	</update>
</mapper>