<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vipshop.crawler.price.dao.PricingBrandMonitorDetailDao">
	<resultMap type="PricingBrandMonitorDetailDto" id="RM_PricingBrandMonitorDetailDto">
		<result column="id" property="id"/>
		<result column="monitor_id" property="monitorId"/>
		<result column="data_date" property="dataDate"/>
		<result column="category_name" property="categoryName"/>
		<result column="brand_name" property="brandName"/>
		<result column="pt_brand_name" property="ptBrandName"/>
		<result column="sell_date_start" property="sellDateStart"/>
		<result column="sell_date_end" property="sellDateEnd"/>
		<result column="site_id" property="siteId"/>
		<result column="vname" property="vname"/>
		<result column="vcode" property="vcode"/>
		<result column="vgood_code" property="vgoodCode"/>
		<result column="vprice" property="vprice"/>
		<result column="oName" property="oname"/>
		<result column="ocode" property="ocode"/>
		<result column="oprice" property="oprice"/>
		<result column="oUrl" property="ourl"/>
		<result column="oImgUrl" property="oimgurl"/>
		<result column="oShopName" property="oshopname"/>
		<result column="score" property="score"/>
		<result column="oupdate_time" property="oupdateTime"/>
		<result column="is_delete" property="isDelete"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
	</resultMap>
	<sql id="PricingBrandMonitorDetailDto-coloumn-list">
		id
		,monitor_id
		,data_date
		,category_name
		,brand_name
		,pt_brand_name
		,sell_date_start
		,sell_date_end
		,site_id
		,vname
		,vcode
		,vgood_code
		,vprice
		,oName
		,ocode
		,oprice
		,oUrl
		,oImgUrl
		,oShopName
		,score
		,oupdate_time
		,is_delete
		,create_time
		,update_time
	</sql>
	<insert id="batchInsert" parameterType="PricingBrandMonitorDetailDto">
		INSERT INTO T_PRICING_BRAND_MONITOR_DETAIL(
		<include refid="PricingBrandMonitorDetailDto-coloumn-list" />
		)
		VALUES
		<foreach collection="list" item="item" index="index"
			separator=",">
			(#{item.monitorId},#{item.dataDate},#{item.categoryName},#{item.brandName},#{item.ptBrandName},#{item.sellDateStart},#{item.sellDateEnd},#{item.siteId},#{item.vname},#{item.vcode},#{item.vgoodCode},#{item.vprice},#{item.oname},#{item.ocode},#{item.oprice},#{item.ourl},#{item.oimgurl},#{item.oshopname},#{item.score},#{item.oupdateTime},#{item.isDelete},#{item.createTime},#{item.updateTime})
		</foreach>
	</insert>
	<update id="batchUpdate" parameterType="PricingBrandMonitorDetailDto">
		UPDATE T_PRICING_BRAND_MONITOR_DETAIL SET
		id=#{id},
		monitor_id=#{monitorId},
		data_date=#{dataDate},
		category_name=#{categoryName},
		brand_name=#{brandName},
		pt_brand_name=#{ptBrandName},
		sell_date_start=#{sellDateStart},
		sell_date_end=#{sellDateEnd},
		site_id=#{siteId},
		vname=#{vname},
		vcode=#{vcode},
		vgood_code=#{vgoodCode},
		vprice=#{vprice},
		oName=#{oname},
		ocode=#{ocode},
		oprice=#{oprice},
		oUrl=#{ourl},
		oImgUrl=#{oimgurl},
		oShopName=#{oshopname},
		score=#{score},
		oupdate_time=#{oupdateTime},
		is_delete=#{isDelete},
		create_time=#{createTime},
		update_time=#{updateTime}
		WHERE ID=#{id}
	</update>
</mapper>