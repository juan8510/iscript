<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vipshop.dp.core.productdatamgr.dao.PricingShopDao">
	<resultMap type="PricingShopDto" id="RM_PricingShopDto">
		<result column="id" property="id"/>
		<result column="shop_name" property="shopName"/>
		<result column="shop_url" property="shopUrl"/>
		<result column="shop_type" property="shopType"/>
		<result column="shop_code" property="shopCode"/>
		<result column="site_id" property="siteId"/>
		<result column="lastest_start_time" property="lastestStartTime"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
	</resultMap>
	<sql id="PricingShopDto-coloumn-list">
		id
		,shop_name
		,shop_url
		,shop_type
		,shop_code
		,site_id
		,lastest_start_time
		,create_time
		,update_time
	</sql>
	<insert id="batchInsert" parameterType="PricingShopDto">
		INSERT INTO T_PRICING_SHOP(
		<include refid="PricingShopDto-coloumn-list" />
		)
		VALUES
		<foreach collection="list" item="item" index="index"
			separator=",">
			(#{item.shopName},#{item.shopUrl},#{item.shopType},#{item.shopCode},#{item.siteId},#{item.lastestStartTime},#{item.createTime},#{item.updateTime})
		</foreach>
	</insert>
	<update id="batchUpdate" parameterType="PricingShopDto">
		UPDATE T_PRICING_SHOP SET
		id=#{id},
		shop_name=#{shopName},
		shop_url=#{shopUrl},
		shop_type=#{shopType},
		shop_code=#{shopCode},
		site_id=#{siteId},
		lastest_start_time=#{lastestStartTime},
		create_time=#{createTime},
		update_time=#{updateTime}
		WHERE ID=#{id}
	</update>
</mapper>