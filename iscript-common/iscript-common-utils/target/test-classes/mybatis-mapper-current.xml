<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lezo.iscript.yeam.tasker.dto.TaskConfigDto">
	<resultMap type="TaskConfigDto" id="RM_TaskConfigDto">
		<result column="ID" property="id"/>
		<result column="TYPE" property="type"/>
		<result column="CONFIG" property="config"/>
		<result column="MODE" property="mode"/>
		<result column="STATUS" property="status"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_TIME" property="updateTime"/>
	</resultMap>
	<sql id="TaskConfigDto-coloumn-list">
		TYPE
		,CONFIG
		,MODE
		,STATUS
		,CREATE_TIME
		,UPDATE_TIME
	</sql>
	<insert id="insert-list-TaskConfigDto" parameterType="TaskConfigDto">
		INSERT INTO T_TASK_CONFIG(
		<include refid="TaskConfigDto-coloumn-list" />
		)
		VALUES
		<foreach collection="list" item="item" index="index"
			separator=",">
			(#{item.type},#{item.config},#{item.mode},#{item.status},#{item.createTime},#{item.updateTime})
		</foreach>
	</insert>
	<update id="update-one-TaskConfigDto" parameterType="TaskConfigDto">
		UPDATE T_TASK_CONFIG SET
		TYPE=#{type},
		CONFIG=#{config},
		MODE=#{mode},
		STATUS=#{status},
		CREATE_TIME=#{createTime},
		UPDATE_TIME=#{updateTime}
		WHERE ID=#{id}
	</update>
</mapper>